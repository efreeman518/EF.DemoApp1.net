<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="EFGithub" value="https://nuget.pkg.github.com/your-org/index.json" />
  </packageSources>

  <!-- Package Source Mapping to satisfy NU1507 when using central package management -->
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <!-- default: most packages -->
      <package pattern="*" />
    </packageSource>
    <packageSource key="EFGithub">
      <!-- map specific EF packages or your private packages to the GitHub source -->
      <package pattern="Package.*" />
      <package pattern="Package.Infrastructure.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>